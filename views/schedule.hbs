<p class="hidden" data-id="{{deviceID}}" id="deviceId">{{deviceID}}</p>
<div class="feed-container">
    <h3>First Feed Settings</h3>
    <p>Chose Time</p>
    <form id="firstFeed">
        <div class="input-container">
            <label for="">Hour</label>
            <img src="/image/arrow.png" alt="arrowup" class="arrow">
            <input type="number" min="0" max="24" value="00" name="hour">
            <img src="/image/arrow.png" alt="arrowup" class="arrow arrowdown">
        </div>
        <div class="input-container">
            <h1 class="devider">:</h1>
        </div>
        <div class="input-container">
            <label for="">Minutes</label>
            <img src="/image/arrow.png" alt="arrowup" class="arrow">
            <input type="number" min="0" max="59" value="00" name="minutes">
            <img src="/image/arrow.png" alt="arrowup" class="arrow arrowdown">
        </div>
    </form>
</div>

<div class="feed-container">
    <h3>Second Feed Settings</h3>
    <p>Chose Time</p>
    <form id="secondFeed">
        <div class="input-container">
            <label for="">Hour</label>
            <img src="/image/arrow.png" alt="arrowup" class="arrow">
            <input type="number" min="0" max="24" value="00" name="hour">
            <img src="/image/arrow.png" alt="arrowup" class="arrow arrowdown">
        </div>
        <div class="input-container">
            <h1 class="devider">:</h1>
        </div>
        <div class="input-container">
            <label for="">Minutes</label>
            <img src="/image/arrow.png" alt="arrowup" class="arrow">
            <input type="number" min="0" max="59" value="00" name="minutes">
            <img src="/image/arrow.png" alt="arrowup" class="arrow arrowdown">
        </div>
    </form>
</div>

<p class="submit" id="submit">Submit</p>
